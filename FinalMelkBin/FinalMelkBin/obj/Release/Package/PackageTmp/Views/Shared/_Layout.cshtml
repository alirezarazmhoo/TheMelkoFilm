    <link href="~/vendors/bower_components/Ionicons/css/ionicons.min.css" rel="stylesheet" />
    <!-- Theme style -->
    <link href="~/vendors/bower_components/AdminLTE.css" rel="stylesheet" />
    <link href="~/vendors/bower_components/_all-skins.min.css" rel="stylesheet" />
