    @RenderSection("header", false)
</head>
<body class="hold-transition skin-blue sidebar-mini box-body" style="font-size: medium;">
    <div class="wrapper">
        <header class="main-header">
            <!-- Logo -->
            <a href="index2.html" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini">پنل</span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><b>پنل مدیریت</b></span>
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>

                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        <li class="dropdown user user-menu">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <i class="fa fa-user"></i>
                               
                                <span class="hidden-xs">@User.Identity.Name  خوش آمدید</span>
                            </a>
                            <ul class="dropdown-menu">
                                <!-- User image -->
                                <li class="user-header">
                                    <i style="font-size: 40px" class="fa fa-user"></i>

                                    <p>
                                        پنل مدیریتی ملکوفیلم
                                        <small>مدیریت کل سایت</small>
                                    </p>
                                </li>
                                <!-- Menu Body -->
                                <!-- Menu Footer-->
                                <li class="user-footer">

                                    <div class="pull-left">

                                        <a type="submit" href="~/Admin/logOut" class="btn btn-default btn-flat">خروج</a>

                                    </div>
                                </li>
                            </ul>
                        </li>
                        <!-- Control Sidebar Toggle Button -->
                    </ul>
                </div>
            </nav>
        </header>
        <!-- right side column. contains the logo and sidebar -->
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
                <!-- Sidebar user panel -->
                <div class="user-panel">
                    <div class="pull-right image">
                        <i style="font-size: 40px;color:white" class="fa fa-user"></i>
                    </div>
                    <div class="pull-right info">
                        <p>@User.Identity.Name</p>
                        <a href="#"><i class="fa fa-circle text-success"></i> آنلاین</a>
                    </div>
                </div>
                <!-- search form -->
                <!-- /.search form -->
                <!-- sidebar menu: : style can be found in sidebar.less -->
                <ul class="sidebar-menu" data-widget="tree">
                    <li class="header">منو</li>


                    <li class="treeview">
                        <a href="#">
                            <i class="glyphicon glyphicon-duplicate"></i>
                            <span> اطلاعات پایه</span>

                            <span class="pull-left-container">
                                <i class="fa fa-angle-right pull-left"></i>
                            </span>

                        </a>

                        <ul class="treeview-menu">
                            @if (User.IsInRole("ShowCity") || User.IsInRole("SuperAdmin"))
                            {

                                <li><a href="~/Admin/AddCity"><i class="fa fa-circle-o"></i>استان</a></li>
                            }

                            @if (User.IsInRole("ShowSubCity") || User.IsInRole("SuperAdmin"))
                            {
                                <li><a href="~/Admin/AddSubCity"><i class="fa fa-circle-o"></i>شهرستان</a></li>

                            }


                        </ul>




                    </li>

                    <li class="treeview">

                        <a href="#">
                            <i class="glyphicon glyphicon-home"></i>
                            <span>املاک</span>
                            <span class="pull-left-container">
                                <i class="fa fa-angle-right pull-left"></i>
                            </span>
                        </a>


                        @*<a href="#"><i class="fa fa-angle-right pull-left"></i><span> املاک</span></a>
            <span class="pull-left-container">
                <i class="fa fa-angle-right pull-left"></i>
            </span>*@
                        <ul class="treeview-menu">
                            @if (User.IsInRole("ShowListMelk") || User.IsInRole("SuperAdmin"))
                            {
                                <li><a href="~/Admin/listMelk"><i class="fa fa-circle-o"></i>لیست ملک ها</a></li>
                            }
                            @if (User.IsInRole("ShowMelkCategori") || User.IsInRole("SuperAdmin"))
                            {
                                <li><a href="~/Admin/AddCategori"><i class="fa fa-circle-o"></i>دسته ها</a></li>
                            }
                            @if (User.IsInRole("ShowMelkSubCategori") || User.IsInRole("SuperAdmin"))
                            {
                                <li><a href="~/Admin/AddSubCategori"><i class="fa fa-circle-o"></i>زیردسته اول</a></li>
                            }


                            @if (User.IsInRole("ShowMelkChildSubCategori") || User.IsInRole("SuperAdmin"))
                            {

                                <li><a href="~/Admin/AddChildSubCategori"><i class="fa fa-circle-o"></i>زیردسته دوم</a></li>
                            }
                            @if (User.IsInRole("ShowDateMelk") || User.IsInRole("SuperAdmin"))
                            {
                                <li><a href="~/Admin/ListDate"><i class="fa fa-circle-o"></i>مدیریت سال ساخت</a></li>
                            }
                        </ul>
                        @*<ul class="treeview-menu" style="display: block;">

            </ul>*@


                    </li>

                    <li class="treeview ">
                        <a href="#">
                            <i class="glyphicon glyphicon-briefcase"></i>
                            <span> مشاغل</span>
                            <span class="pull-left-container">
                                <i class="fa fa-angle-right pull-left"></i>
                            </span>
                        </a>

                        <ul class="treeview-menu">
                            @if (User.IsInRole("ShowListJob") || User.IsInRole("SuperAdmin"))
                            {
                                <li><a href="~/Admin/listJobs"><i class="fa fa-circle-o"></i> لیست شغل ها</a></li>
                            }
                            @if (User.IsInRole("ShowJobCategori") || User.IsInRole("SuperAdmin"))
                            {
                                <li><a href="~/Admin/JobCategori"><i class="fa fa-circle-o"></i>مدیریت دسته مشاغل</a></li>
                            }
                            @if (User.IsInRole("ShowJobSubCategori") || User.IsInRole("SuperAdmin"))
                            {
                                <li><a href="~/Admin/JobSubCategori"><i class="fa fa-circle-o"></i>مدیریت زیر دسته اول مشاغل</a></li>
                            }
                            @if (User.IsInRole("ShowJobChildSubCategori") || User.IsInRole("SuperAdmin"))
                            {
                                <li><a href="~/Admin/JobChildSubCategori"><i class="fa fa-circle-o"></i>مدیریت زیر دسته دوم مشاغل</a></li>
                            }
                        </ul>



                        @*<ul class="treeview-menu" style="display: block;">


            </ul>
            <ul class="treeview-menu" style="display: block;">


            </ul>*@

                    </li>
                    @if (User.IsInRole("ShowEquipment") || User.IsInRole("SuperAdmin"))
                    {
                    <li class="treeview ">
                        <a href="#">
                            <i class="glyphicon glyphicon-scissors"></i><span> لوازم دسته دوم</span> <span class="pull-left-container">
                                <i class="fa fa-angle-right pull-left"></i>
                            </span>
                        </a>

                        <ul class="treeview-menu">
                            @if (User.IsInRole("ShowEquipment") || User.IsInRole("SuperAdmin"))
                            {
                                <li><a href="~/Admin/listEquipments"><i class="fa fa-circle-o"></i>لیست  لوازم</a></li>
                            }
                            @if (User.IsInRole("ShowEquipmentCategori") || User.IsInRole("SuperAdmin"))
                            {
                                <li><a href="~/Admin/equipmentCategori"><i class="fa fa-circle-o"></i>مدیریت گروه بندی لوازم</a></li>
                            }
                            @if (User.IsInRole("ShowSubEquipmentCategori") || User.IsInRole("SuperAdmin"))
                            {
                                <li><a href="~/Admin/equipmentSubCategori"><i class="fa fa-circle-o"></i>مدیریت زیر گروه اول لوازم</a></li>
                            }
                            @if (User.IsInRole("ShowEquipmentChildSubCategori") || User.IsInRole("SuperAdmin"))
                            {
                                <li><a href="~/Admin/equipmentChildSubCategori"><i class="fa fa-circle-o"></i>مدیریت زیر گروه دوم لوازم</a></li>
                            }
                            </ul>
                        


                    </li>
                        }
                        @if (User.IsInRole("SuperAdmin"))
                        {
                            <li class="treeview ">
                                <a href="#">
                                    <i class="glyphicon glyphicon-user"></i><span> ادمین ها</span>
                                    <span class="pull-left-container">
                                        <i class="fa fa-angle-right pull-left"></i>
                                    </span>
                                </a>

                                <ul class="treeview-menu">
                                    <li><a href="~/Admin/listAdmin"><i class="fa fa-circle-o"></i>فعالت ادمین در خصوص املاک</a></li>

                                    <li><a href="~/Admin/LstJobActivityAdmin"><i class="fa fa-circle-o"></i>فعالت ادمین در خصوص مشاغل</a></li>
                                    <li><a href="~/Admin/LstEquipmentActivityAdmin"><i class="fa fa-circle-o"></i>فعالیت ادمین در لوازم دسته دوم</a></li>
                                </ul>

                            </li>
                            <li class="treeview ">
                                <a href="#">
                                    <i class="glyphicon glyphicon-tasks"></i><span> مدیریت کاربران</span>
                                    <span class="pull-left-container">
                                        <i class="fa fa-angle-right pull-left"></i>
                                    </span>
                                </a>
                                <ul class="treeview-menu">
                                    <li><a href="~/Admin/AdminUser"><i class="fa fa-circle-o"></i>اسامی و مدیریت</a></li>

                                </ul>

                            </li>
                        }






                        <li>

                            <a style="cursor:pointer" href="~/Admin/logOut">

                                <i class="fa fa-sign-out"></i><span>خروج</span>

                            </a>
                        </li>

                    </ul>

            </section>
            <!-- /.sidebar -->
        </aside>
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper box-body " id="mypaging">
