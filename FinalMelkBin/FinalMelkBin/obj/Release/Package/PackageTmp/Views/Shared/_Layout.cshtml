            @*<section class="content-header">
                    @RenderSection("headerContent", false)
                </section>*@
            <!-- Main content -->
            <section class="content box-body">
                @RenderBody()
            </section>
            <!-- /.content -->
            <!-- /.content-wrapper -->
            @*<footer class="main-footer text-left"></footer>*@
        </div>

    </div>


